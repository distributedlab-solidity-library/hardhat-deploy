{
  "name": "@dlsl/hardhat-migrate",
  "version": "1.2.0",
  "description": "This package facilitates automatic deployment and verification of Solidity Smart Contracts",
  "scripts": {
    "install": "npx husky install",
    "compile": "tsc --build .",
    "build-local": "npm run compile && npm run lint-fix && cp -r ./README.md ./LICENSE ./dist ./src ./publish",
    "lint-fix": "prettier --write \"./**/*.ts\"",
    "test": "echo \"No test specified\"",
    "publish-to-npm": "npm run compile && npm run lint-fix && bash ./scripts/publish.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/distributedlab-solidity-library/hardhat-migrate.git"
  },
  "keywords": [
    "ethereum",
    "solidity",
    "smart-contracts",
    "hardhat",
    "hardhat-plugin",
    "etherscan",
    "distributedlab"
  ],
  "author": "Distributed Lab",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/distributedlab-solidity-library/hardhat-deploy/issues"
  },
  "homepage": "https://github.com/distributedlab-solidity-library/hardhat-deploy#readme",
  "dependencies": {
    "@nomiclabs/hardhat-etherscan": "3.1.3",
    "@truffle/deployer": "3.2.56",
    "@truffle/reporters": "1.1.2",
    "bignumber.js": "^9.1.0",
    "hardhat": "^2.10.0",
    "lodash": "^4.17.21",
    "web3": "^1.7.5"
  },
  "peerDependencies": {
    "hardhat": "^2.10.0"
  },
  "devDependencies": {
    "husky": "^7.0.2",
    "prettier": "^2.7.1",
    "ts-node": "^8.1.0",
    "typescript": "~4.5.2"
  }
}
