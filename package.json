{
  "name": "@solarity/hardhat-migrate",
  "version": "1.5.8",
  "description": "Automatic deployment and verification of smart contracts",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/src/",
    "src/",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dl-solarity/hardhat-migrate.git"
  },
  "keywords": [
    "ethereum",
    "solidity",
    "smart-contracts",
    "hardhat",
    "hardhat-plugin",
    "etherscan",
    "distributedlab",
    "solarity"
  ],
  "author": "Distributed Lab",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dl-solarity/hardhat-migrate/issues"
  },
  "homepage": "https://github.com/dl-solarity/hardhat-migrate#readme",
  "scripts": {
    "install": "npx husky install",
    "build": "tsc --build .",
    "test": "mocha --recursive \"test/**/*.ts\" --exit",
    "sol-test": "cd sample-project/ && npx hardhat test && cd ../",
    "lint-fix": "prettier --write \"./**/*.ts\" && prettier --write \"./**/*.js\"",
    "publish-to-npm": "npm run build && npm run lint-fix && npm publish ./ --access public"
  },
  "dependencies": {
    "@nomiclabs/hardhat-etherscan": "3.1.3",
